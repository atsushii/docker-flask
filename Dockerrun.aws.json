{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "web",
            "image": "atsushiiii/multi-server",
            "hostname": "web",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "atsushiiii/multi-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostport": 80,
                    "containerPort": 80
                }
            ],
            "links": ["web"],
            "memory": 128
        }
    ]
}